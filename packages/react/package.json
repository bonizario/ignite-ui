{
  "name": "@bonizario-ignite-ui/react",
  "version": "1.0.0",
  "description": "Ignite UI react components",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.tsx --format esm,cjs --dts --external react",
    "dev": "tsup src/index.tsx --format esm,cjs --dts --external react --watch",
    "lint": "eslint 'src/**/*.{ts,tsx,d.ts}' --fix"
  },
  "keywords": [
    "ignite-ui",
    "react",
    "components"
  ],
  "author": "bonizario",
  "license": "MIT",
  "dependencies": {
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@stitches/react": "^1.2.8",
    "phosphor-react": "^1.4.1"
  },
  "devDependencies": {
    "@bonizario-ignite-ui/eslint-config": "workspace:*",
    "@bonizario-ignite-ui/tokens": "workspace:*",
    "@bonizario-ignite-ui/ts-config": "workspace:*",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "react": "^18.2.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  }
}
